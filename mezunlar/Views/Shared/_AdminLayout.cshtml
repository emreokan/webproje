<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="stylesheet" href="~/Blog/css/style.css">
    <title>Home Page</title>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script src="~/Blog/script/script.js" type="text/javascript"></script>
    <script src="https://code.jquery.com/jquery-1.11.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.min.js"></script>
    <script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/additional-methods.min.js"></script>

</head>

<body>
    <header>
        <nav class="navbar bg-primary navbar-expand-lg navbar-dark">

            <div class="container">
                <a href="/" class="navbar-brand">
                    <img id="logom" src="~/Blog/img/yok.jpeg" alt="" height="50" width="100">
                </a>

                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse">
                    <span class="navbar-toggler-icon"></span>
                </button>
              
                <div id="navbarCollapse" class="collapse navbar-collapse">
                    <ul class="navbar-nav ml-auto btn-group" role="group">
                        <li class="nav-item btn btn-primary">
                            <a href="@Url.Action("Index", "Blogs")" class="nav-link active">
                                Bloglar
                            </a>
                        </li>
                        <li class="nav-item btn btn-primary">
                            <a href="@Url.Action("Index", "Users")" class="nav-link active">
                                Kullanıcı
                            </a>
                        </li>
                        <li class="nav-item btn btn-primary">
                            <a href="@Url.Action("Index", "Albums")" class="nav-link active">
                                Albüm
                            </a>
                        </li>
                        <li class="nav-item btn btn-primary">
                            <a href="@Url.Action("Index", "Categories")" class="nav-link active">
                                Katogoriler
                            </a>
                        </li>
                        <li class="nav-item btn btn-primary">
                            <a href="@Url.Action("Index", "Etkinliklers")" class="nav-link active">
                                Etkinlikler
                            </a>
                        </li>
                        <li class="nav-item btn btn-primary">
                            <a href="@Url.Action("Index", "Jobs")" class="nav-link active">
                                iş
                            </a>
                        </li>
                        @Html.Partial("_LoginPartial")

                    </ul>
                </div>

            </div>
        </nav>
    </header>

    <main>
        <div class="container">
            @RenderBody()
        </div>
    </main>
    @*<footer class="py-3 bg-primary text-white text-center sticky-bottom">
        <div class="container">  Copyright 2019  <small>by ufuk uysal</small></div>
    </footer>*@





</body>

</html>
